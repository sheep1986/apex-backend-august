{
    "version": 2,
    "builds": [
          {
                  "src": "dist/server.js",
                  "use": "@vercel/node"
          }
    ],
    "routes": [
          {
                  "src": "/(.*)",
                  "dest": "dist/server.js"
          }
    ],
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "env": {
          "NODE_ENV": "production"
    }
}
